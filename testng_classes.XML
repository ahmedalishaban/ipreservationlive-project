<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Login">
    <parameter name="URL" value="https://testui.ipreservationlive.com/admin/login"/>
    <test name="Login with Valid username and password">
        <parameter name="username" value="check123"/>
        <parameter name="password" value="helloworld"/>
        <classes>
            <class name="AdminTest.Dashboard.TC01_login"/>
        </classes>
    </test>

    <test name="Login with InValid username and password">
        <parameter name="username" value="check12"/>
        <parameter name="password" value="helloworl"/>
        <classes>
            <class name="AdminTest.Dashboard.TC01_login"/>
        </classes>
    </test>
</suite>